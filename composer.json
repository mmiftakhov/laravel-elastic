{
    "name": "maratmiftahov/laravel-elastic",
    "description": "Laravel wrapper for Elasticsearch",
    "version": "0.1.0",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Maratmiftahov\\LaravelElastic\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Maratmiftahov\\LaravelElastic\\ElasticServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Marat Miftakhov",
            "email": "m.miftahov@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "elasticsearch/elasticsearch": "^8.18"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
