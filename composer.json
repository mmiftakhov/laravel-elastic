{
    "name": "maratmiftahov/laravel-elastic",
    "description": "Simple and powerful Elasticsearch integration for Laravel with automatic indexing and multilingual support",
    "version": "0.5.2",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marat Miftakhov",
            "email": "m.miftahov@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
    "laravel/framework": "^10.0|^11.0",
    "elasticsearch/elasticsearch": "^8.18",
    "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0 || ^8.0",
        "phpunit/phpunit": "^10.5 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Maratmiftahov\\LaravelElastic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maratmiftahov\\LaravelElastic\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
      "providers": [
        "Maratmiftahov\\LaravelElastic\\ElasticServiceProvider"
      ],
            "aliases": {}
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
